{
  "list": [
    {{#repeat min=5 max=10}}
      {
        "id": "{{int 1 1000}}",
        "title": "{{lorem min=1 max=3}}",
        "artist": "{{firstName}} {{lastName}}",
        "picture": {{int 10000000 99999999 }},
        "type": "{{random 'Electro' 'Rock' 'Pop' 'Alternative' 'Classic' 'Rap'}}",
        "category": "{{random 'playlist' 'album'}}",
        "publication": "{{date '2010' '2025' 'YYYY-MM-DD'}}",
        "musics": [
        {{#repeat min=3 max=10}}
          {{int 1000 2000}},
        {{/repeat}}
        ]
      }
    {{/repeat}}
  ],
  "music" : [
    {{#repeat min=5 max=10}}
      {
        "id": "{{int 1000 2000}}",
        "title": "{{lorem min=1 max=3}}",
        "artist": "{{firstName}} {{lastName}}",
        "album": {{int 1 1000}},
        "picture": {{int 10000000 99999999 }},
        "type": "{{random 'Electro' 'Rock' 'Pop' 'Alternative' 'Classic' 'Rap'}}",
        "publication": "{{date '2010' '2025' 'YYYY-MM-DD'}}"
      }
    {{/repeat}}
  ]
}